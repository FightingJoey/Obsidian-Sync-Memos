	"keywords": [
		"obsidian",
		"plugin",
		"memos",
		"sync",
		"diary"
	],
	"author": "DevQiaoYu",
	"license": "MIT",
	"devDependencies": {
		"@types/axios": "^0.14.4",
		"@types/moment": "^2.13.0",
		"@types/node": "^16.18.126",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"axios": "^1.6.5"
	}
}